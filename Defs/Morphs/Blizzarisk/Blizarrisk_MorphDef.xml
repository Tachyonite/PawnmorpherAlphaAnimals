<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef>
		<defName>BlizzariskMorph</defName>
		<label>blizzariskmorph</label>
		<description>Blizzariskmorphs are unusually fuzzy arachnoid beings, with a venomous bite and an uncanny ability to produce morphsilk. Just don't ask where it comes from.</description>
		<race>AA_Blizzarisk</race>
		<classification>Animal</classification>
		<categories>
			<li>Temperate</li>
		</categories>
		<fullTransformation>PawnmorphBlizzariskTF</fullTransformation>
		<partialTransformation>PawnmorphBlizzariskTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(255,255,255)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>BlizzariskTransformationMemory</transformationMemory>
			<revertedMemory>BlizzariskRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>