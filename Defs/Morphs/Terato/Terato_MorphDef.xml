<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef>
		<defName>TeratoMorph</defName>
		<label>teratomorph</label>
		<description>Teratomorphs are slug-like, parasitic beings, with a mutagenic bite and a horrendous method of producing more Teratos.</description>
		<race>AA_TeratogenicOriginator</race>
		<classification>Animal</classification>
		<categories>
			<li>Temperate</li>
		</categories>
		<fullTransformation>PawnmorphTeratoTF</fullTransformation>
		<partialTransformation>PawnmorphTeratoTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(209,188,167,150)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>TeratoTransformationMemory</transformationMemory>
			<revertedMemory>TeratoRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>