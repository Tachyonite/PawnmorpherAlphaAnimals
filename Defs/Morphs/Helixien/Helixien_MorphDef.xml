			<li>Temperate</li>
		</categories>
		<fullTransformation>PawnmorphHelixienTF</fullTransformation>
		<partialTransformation>PawnmorphHelixienTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(168,106,86)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
			<explicitHybridRace>HelixienMorph</explicitHybridRace>
			<transferHumanBodyAddons>true</transferHumanBodyAddons>
			<requiredMutations>
				<li>EtherHelixienTail</li>
			</requiredMutations>
		</raceSettings>
		<transformSettings>
			<transformationMemory>HelixienTransformationMemory</transformationMemory>
			<revertedMemory>HelixienRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>