		<fullTransformation>PawnmorphFeraliskTF</fullTransformation>
		<partialTransformation>PawnmorphFeraliskTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(85,73,67)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>FeraliskTransformationMemory</transformationMemory>
			<revertedMemory>FeraliskRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
