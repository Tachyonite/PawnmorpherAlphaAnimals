		<classification>Animal</classification>
		<categories>
			<li>Temperate</li>
		</categories>
		<fullTransformation>PawnmorphJungleliskTF</fullTransformation>
		<partialTransformation>PawnmorphJungleliskTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(128,138,79)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>JungleliskTransformationMemory</transformationMemory>
			<revertedMemory>JungleliskRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>