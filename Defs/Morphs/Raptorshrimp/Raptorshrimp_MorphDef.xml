<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef>
		<defName>RaptorshrimpMorph</defName>
		<label>Raptorshrimpmorph</label>
		<description>Raptorshrimpmorphs are blade-handed beasts with amazing regenerative powers. You'd think that would describe the Wolverinemorph, but you'd be wrong.</description>
		<race>AA_RaptorShrimp</race>
		<classification>Animal</classification>
		<categories>
			<li>Arid</li>
			<li>Powerful</li>
			<li>Tropical</li>
		</categories>
		<fullTransformation>PawnmorphRaptorshrimpTF</fullTransformation>
		<partialTransformation>PawnmorphRaptorshrimpTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(100,105,105)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
			<explicitHybridRace>RaptorshrimpMorph</explicitHybridRace>
			<transferHumanBodyAddons>true</transferHumanBodyAddons>
			<requiredMutations>
				<li>EtherRaptorshrimpTail</li>
			</requiredMutations>
		</raceSettings>
		<transformSettings>
			<transformationMemory>RaptorshrimpTransformationMemory</transformationMemory>
			<revertedMemory>RaptorshrimpRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>