    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>RaptorshrimpTail</defName>
        <stages>
            <li>
                <label>Shrimp tail</label>
                <description>This thing gets in the way.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Shrimp tail</label>
                <description>I can really glide along with this thing!</description>
                <visible>true</visible>
                <baseMoodEffect>1</baseMoodEffect>
            </li>
            <li>
                <label>Shrimp tail</label>
