<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/ThoughtDef.xsd">
    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>RaptorshrimpTransformationMemory</defName>
        <stages>
            <li>
                <label>morphed</label>
                <description>What the hell am I gonna do with these blade hands?</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>morphed</label>
                <description>Chop chop!</description>
                <baseMoodEffect>+3</baseMoodEffect>
            </li>
            <li>
                <label>morphed</label>
                <description>Noooo! I wanted to be a pianist!</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
            <li>
                <label>morphed</label>
                <description>I feel like a total badass, but it's a little rough not having any fingers.</description>
                <baseMoodEffect>+1</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>RaptorshrimpRevertedMemory</defName>
        <stages>
            <li>
                <label>reverted</label>
                <description>Yes! I can scratch an itch without multiple casualties again!</description>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>I feel so... soft and weak.</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>Piano here I come!</description>
                <baseMoodEffect>10</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>I'm human again? I think I wanted the opposite of this.</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>


    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
