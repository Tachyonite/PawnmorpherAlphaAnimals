                <label>Raptor shrimp face</label>
                <description>My hairstyle! Nooo!</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Raptor shrimp face</label>
                <description>I'm bulletproof!</description>
                <visible>true</visible>
                <baseMoodEffect>2</baseMoodEffect>
            </li>
            <li>
                <label>Raptor shrimp face</label>
                <description>I'm hideous!</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
